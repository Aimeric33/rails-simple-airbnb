<%= simple_form_for flat do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>

  <%= f.input :address %>

  <%= f.input :description %>

  <%= f.input :price_per_night %>

  <%= f.input :number_of_guest %>

  <%= f.input :picture_url %>

  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>
